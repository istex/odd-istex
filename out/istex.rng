      </optional>
   </define>
   <define name="tei_att.duration.iso.attributes">
      <ref name="tei_att.duration.iso.attribute.dur-iso"/>
   </define>
   <define name="tei_att.duration.iso.attribute.dur-iso">
      <optional>
         <attribute name="dur-iso">

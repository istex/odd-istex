      <xs:annotation>
        <xs:documentation>spécifie le nom de la version ou le numéro de la source dont la version traduite a été tirée.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union memberTypes="xs:date xs:gYear xs:gMonth xs:gDay xs:gYearMonth xs:gMonthDay xs:time xs:dateTime"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="tei_att.citing.attributes">
    <xs:attributeGroup ref="tei:tei_att.citing.attribute.unit"/>

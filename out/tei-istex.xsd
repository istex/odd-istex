  <xs:attributeGroup name="tei_att.styleDef.attribute.scheme">
    <xs:attribute name="scheme">
      <xs:annotation>
        <xs:documentation>identifie la langue employée pour décrire le rendu</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="css">
            <xs:annotation>
              <xs:documentation>langage CSS (Cascading Stylesheet )</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="xslfo">
            <xs:annotation>
              <xs:documentation>Langage XSL (Extensible Stylesheet )Formatting Objects</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="free">
            <xs:annotation>
              <xs:documentation>description en texte libre non structuré.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="other">
            <xs:annotation>
              <xs:documentation>langue de description de l'interprétation définie par l'utilisateur</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>

    <text>
        <front>
            <divGen type="toc"/>
        </front>
        <body>
            <p>Spécification d'un schéma ISTEX pour les sorties Tei</p>
            <schemaSpec ident="istexschema" docLang="fr" 
                prefix="tei_" xml:lang="fr" 
                start="teiCorpus TEI standOff"
                source="https://raw.githubusercontent.com/laurentromary/stdfSpec/AnnArbor/Specification/standoff-proposal_doc.xml">
                <moduleRef key="core"/>
                <moduleRef key="tei" except="fsdDecl facsimile"/>
                <moduleRef key="header"
                    except="cRefPattern calendar calendarDesc catDesc catRef correction correspAction correspContext correspDesc editorialDecl geoDecl handNote hyphenation interpretation listChange listPrefixDef normalization prefixDef principal projectDesc punctuation quotation refState refsDecl samplingDecl scriptNote segmentation sponsor stdVals styleDefDecl tagUsage tagsDecl typeNote xenoData"/>
                <moduleRef key="namesdates"
                    except="age birth bloc climate death district education faith floruit geo geogFeat langKnowledge langKnown listEvent listNym listPerson listRelation nationality nym occupation offset population relation residence sex socecStatus terrain trait"/>
                <moduleRef key="spoken" except="broadcast equipment incident kinesic pause recording recordingStmt scriptStmt shift transcriptionDesc u vocal writing"/>
                <moduleRef key="textstructure"
                    except="argument byline closer dateline div1 div2 div3 div4 div5 div6 div7 docEdition docImprint epigraph group imprimatur opener postscript salute signed titlePage titlePart trailer"/>
                <moduleRef key="figures"/>
                <moduleRef key="transcr"/>
                <moduleRef key="linking"/>
                <moduleRef key="standOff"/>
                <elementSpec ident="bibl" module="core" mode="change">
                    <classes mode="replace">
                        <memberOf key="model.biblLike"/>
                        <memberOf key="model.biblPart"/>
                        <memberOf key="att.declarable"/>
                        <memberOf key="att.docStatus"/>
                        <memberOf key="att.global"/>
                        <memberOf key="att.sortable"/>
                        <memberOf key="att.timed" mode="add"/>
                        <memberOf key="att.typed"/>
                    </classes>
                    <remarks>
                        <p>Dans Istex on utilise plutôt biblStruct</p>
                    </remarks>
                </elementSpec>

                <!-- abstract -->
                <elementSpec ident="abstract" mode="change">
                    <content>
                        <alternate minOccurs="1" maxOccurs="unbounded">
                            <classRef key="model.pLike"/>
                            <classRef key="model.listLike"/>
                            <classRef key="model.headLike"/>
                        </alternate>
                    </content>
                    <exemplum xml:lang="fr">
                        <p>Cas où un abstract contient un titre</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples"><abstract><head>Méthode</head><p>Contenu de la méthode.</p><head>Expérience</head><p>Contenu de
                            l'expérience.</p></abstract></egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>head</gi> dans <gi>abstract</gi></p>
                    </remarks>
                </elementSpec>

                <elementSpec ident="listAnnotation" mode="change" ns="http://www.example.org/ns/nonTEI">
                    <desc/>
                    <classes>
                        <memberOf key="model.divPart.spoken"/>
                        <memberOf key="att.typed"/>
                    </classes>
                    <content xmlns:rng="http://relaxng.org/ns/structure/1.0"> </content>
                    <exemplum xml:lang="fr">
                        <p>L'élément listAnnotation regroupe des <gi>annotationBlock</gi></p>
                        <p>L'élément annotationBlock regroupe un enrichissement (entité nommée, catégorisation, indexation) et les éléments qui lui correspondent comme les <gi>term</gi> et les
                                <gi>fs</gi></p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="date" xml:lang="fr">
                                <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                    <date change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://date-entity.lod.istex.fr">
                                        <term>2000</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="5"/>
                                            </f>
                                        </fs>
                                    </date>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="rd-nb">
                                <annotationBlock corresp="abstract" xmls="https//www.tei-c.org/ns/1.0">
                                    <keywords change="#rd-nb" resp="#istex-rd" scheme="http://inist-category.lod.istex.fr">
                                        <term cert="0.651637563297372" key="refCode : STM" level="1">SCIENCES APPLIQUEES, TECHNOLOGIE ET MEDECINE</term>
                                        <term cert="0.5541006144477459" key="refCode : 001" level="2">SCIENCES EXACTES ET TECHNOLOGIE</term>
                                        <term cert="0.20272618300842637" key="refCode : 001E" level="3">TERRE, OCEAN, ESPACE</term>
                                        <term cert="0.31605416143215126" key="refCode : 001E02" level="4">GEOPHYSIQUE EXTERNE</term>
                                    </keywords>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="rd-teeft">
                                <annotationBlock corresp="text" xmls="https//www.tei-c.org/ns/1.0">
                                    <keywords change="#istex-rd" resp="#istex-rd">
                                        <term>
                                            <term>prepulse inhibition</term>
                                            <fs type="statistics">
                                                <f name="frequency">
                                                    <numeric value="4"/>
                                                </f>
                                                <f name="specificity">
                                                    <numeric value="1"/>
                                                </f>
                                            </fs>
                                        </term>
                                        <term>
                                            <term>decompensated inpatients</term>
                                            <fs type="statistics">
                                                <f name="frequency">
                                                    <numeric value="2"/>
                                                </f>
                                                <f name="specificity">
                                                    <numeric value="0.5"/>
                                                </f>
                                            </fs>
                                        </term>
                                    </keywords>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>annotationBlock</gi> dans <gi>listAnnotation</gi></p>
                    </remarks>
                </elementSpec>
                <!-- annotationBlock -->
                <elementSpec ident="annotationBlock" mode="change">
                    <content>
                        <alternate minOccurs="1" maxOccurs="unbounded">
                            <classRef key="model.divPart.spoken"/>
                            <!--modele pour date-->
                            <classRef key="model.dateLike"/>
                            <classRef key="model.publicationStmtPart.detail"/>
                            <!--modele pour persName-->
                            <classRef key="model.nameLike.agent"/>
                            <classRef key="model.persStateLike"/>
                            <!--modele pour placeName-->
                            <classRef key="model.placeNamePart"/>
                            <classRef key="model.ptrLike"/>
                            <!--modele pour orgName-->
                            <classRef key="model.nameLike.agent"/>
                            <!-- subject -->
                            <elementRef key="keywords"/>
                            <!-- author -->
                            <classRef key="model.respLike"/>
                        </alternate>
                    </content>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <date change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://date-entity.lod.istex.fr">
                                    <term>1997</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="7"/>
                                        </f>
                                    </fs>
                                </date>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff classification alignement avec le hub de métadonnées</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="#subject-01">
                                <keywords resp="#istex" change="#istex-01">
                                    <term>Pharmacologie</term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>date</gi>, <gi>persName</gi>, <gi>orgName</gi>, <gi>placeName</gi>, <gi>geogName</gi>,<gi>ref</gi>, <gi>keywords</gi>, <gi>author</gi>
                            dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- date -->
                <elementSpec ident="date" mode="change">
                    <!-- ajout de fs au niveau de date -->
                    <content>
                        <alternate minOccurs="1" maxOccurs="unbounded">
                            <classRef key="model.featureVal.complex"/>
                            <classRef key=" model.global.meta"/>
                        </alternate>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée Date</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <date change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://date-entity.lod.istex.fr">
                                    <term>1997</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="7"/>
                                        </f>
                                    </fs>
                                </date>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>date</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- persName -->
                <elementSpec ident="persName" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée persName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <persName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://persname-entity.lod.istex.fr">
                                    <term>Timothy Adams</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </persName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>persName</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- orgName type="funder" ou type="provider"-->
                <elementSpec ident="orgName" mode="change">
                    <content>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée orgName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://orgname-entity.lod.istex.fr">
                                    <term>Working Party</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="2"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée orgName type="funder"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://orgnamefunder-entity.lod.istex.fr">
                                    <term>Organization for Pharmaceutical Safety and Research</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée orgName type="provider"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://orgnameprovider-entity.lod.istex.fr">
                                    <term>TOMCAT at the SLS in Villigen, Switzerland</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>orgName</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- placeName -->
                <elementSpec ident="placeName" mode="change">
                    <content>
                        <classRef key="model.placeNamePart"/>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée placeName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <placeName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://placename-entity.lod.istex.fr">
                                    <term>Geneva</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="9"/>
                                        </f>
                                    </fs>
                                </placeName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>placeName</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- geogName -->
                <elementSpec ident="geogName" mode="change">
                    <content>
                        <classRef key="model.placeNamePart"/>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée geogName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <geogName change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://geogname-entity.lod.istex.fr">
                                    <term>Mississippi River</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="2"/>
                                        </f>
                                    </fs>
                                </geogName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>geogName</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- ref type="bibl" ou type="url" ajouter l'attribut type-->
                <elementSpec ident="ref" mode="change">
                    <content>
                        <classRef key="model.ptrLike"/>
                        <macroRef key="macro.paraContent"/>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée ref type="bibl"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <ref type="bibl" change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://refbibl-entity.lod.istex.fr">
                                    <term>Stern et al.</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </ref>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée ref type="url"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <ref type="url" change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://refurl-entity.lod.istex.fr">
                                    <term>http://www.treas.gov/press/releases/js</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="3"/>
                                        </f>
                                    </fs>
                                </ref>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <!-- exemple nature
                    <cite id="n1"><jtl>Nature</jtl>
        <vid>402</vid>, <ppf> 255</ppf>ndash;<ppl>262</ppl>; <cd year="1999">1999</cd></cite>-->
                    <exemplum xml:lang="fr">
                        <p>Cas où un paragraphe contient titre de revue/livre + sa tomaison </p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <p>Originally published as <ref type="cit" xml:id="n1">
                                    <title level="j" type="main">Nature</title>
                                    <biblScope unit="vol">402</biblScope>, <biblScope unit="page" from=" 255"> 255</biblScope>ndash;<biblScope unit="page" to="262">262</biblScope>; </ref>
                            </p>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>biblScope</gi> dans <gi>ref</gi></p>
                    </remarks>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>ref</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- bibl -->
                <elementSpec ident="bibl" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée bibl</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https://wwww.tei-c.org/ns/1.0">
                                <bibl change="#Unitex-3.2.0-alpha" resp="istex-rd" scheme="http://bibl-entity.lod.istex.fr">
                                    <term>GIRODET, Jean (2007). Dictionnaire des pièges et difficults de la langue française, Paris : Bordas</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </bibl>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>geogName</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- SG - keywords hub de mta-->
                <elementSpec ident="keywords" mode="change">
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement subject hub de métadonnées Abes</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="#subject-01">
                                <keywords resp="#abes" change="#abes-01">
                                    <term>Pharmacologie</term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement indexation rd-teeft</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text" xmls="https//www.tei-c.org/ns/1.0">
                                <keywords change="#istex-rd" resp="#istex-rd">
                                    <term>
                                        <term>prepulse inhibition</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="4"/>
                                            </f>
                                            <f name="specificity">
                                                <numeric value="1"/>
                                            </f>
                                        </fs>
                                    </term>
                                    <term>
                                        <term>decompensated inpatients</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="2"/>
                                            </f>
                                            <f name="specificity">
                                                <numeric value="0.5"/>
                                            </f>
                                        </fs>
                                    </term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>keywords</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- SG - author hub de mta-->
                <elementSpec ident="author" mode="change">
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement auteur hub de mtadonnes Abes</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <author>
                                <name type="person" resp="#abes" change="#author-01">Nom Prénom</name>
                                <persName>
                                    <forename type="first" resp="#abes" change="#author-01">Prénom</forename>
                                    <surname resp="#abes" change="#author-01">Nom</surname>
                                </persName>
                            </author>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>author</gi> dans <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>
                <!-- fs -->
                <elementSpec ident="fs" mode="change">
                    <exemplum xml:lang="fr">
                        <p>l'élément <gi>fs</gi>contient </p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples"><fs type="statistics">
                                <f name="frequency">
                                    <numeric value="7"/>
                                </f>
                            </fs>
                        </egXML>
                    </exemplum>
                </elementSpec>
                <!-- f -->
                <elementSpec ident="f" mode="change">
                    <exemplum xml:lang="fr">
                        <p>l'élément <gi>f</gi>contient</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <f name="frequency">
                                <numeric value="2"/>
                            </f>
                        </egXML>
                    </exemplum>
                </elementSpec>
                <!-- numeric -->
                <elementSpec ident="numeric" mode="change">
                    <exemplum xml:lang="fr">
                        <p>l'élément <gi>numeric</gi>contient la valeur de la fréquence</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <f name="frequency">
                                <numeric value="2"/>
                            </f>
                        </egXML>
                    </exemplum>
                </elementSpec>
                
                <!-- ajout d'un attribut level dans <term> -->
                <elementSpec ident="term" mode="change">
                    <attList>
                        <attDef ident="level" mode="add">
                            <desc>Niveau hiérarchique du terme dans le code de classement</desc>
                            <datatype>
                                <dataRef key="teidata.count"/>
                            </datatype>
                        </attDef>
                    </attList>
                </elementSpec>
            </schemaSpec>
        </body>
    </text>
</TEI>

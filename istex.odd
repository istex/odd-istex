        </front>
        <body>
            <p>Spécification d'un schéma ISTEX pour les sorties Tei</p>
            <schemaSpec ident="istexschema" docLang="fr" prefix="tei_" xml:lang="fr"
                start="teiCorpus TEI standOff"
                source="https://raw.githubusercontent.com/laurentromary/stdfSpec/AnnArbor/Specification/standoff-proposal_doc.xml">
                <moduleRef key="core"/>
                <moduleRef key="tei" except="fsdDecl facsimile"/>
                <moduleRef key="header"
                    except="cRefPattern calendar calendarDesc catDesc catRef correspAction correspContext correspDesc editorialDecl geoDecl handNote hyphenation interpretation listChange listPrefixDef normalization prefixDef principal projectDesc punctuation quotation refState refsDecl samplingDecl scriptNote segmentation sponsor stdVals styleDefDecl tagUsage tagsDecl typeNote xenoData"/>
                <moduleRef key="namesdates"
                    except="age birth bloc climate death district education faith floruit geo geogFeat langKnowledge langKnown listEvent listNym listPerson listRelation nationality nym occupation offset population relation residence sex socecStatus terrain trait"/>
                <moduleRef key="spoken"
                    except="broadcast equipment incident kinesic pause recording recordingStmt scriptStmt shift transcriptionDesc u vocal writing"/>
                <moduleRef key="textstructure"
                    except="argument byline closer dateline div1 div2 div3 div4 div5 div6 div7 docImprint epigraph imprimatur opener postscript salute signed trailer"/>
                <moduleRef key="figures"/>
                <moduleRef key="transcr"/>
                <!-- ajout modèle iso-fs pour prendre en charge les <fs> -->
                <moduleRef key="iso-fs"/>
                <moduleRef key="linking"/>
                <moduleRef key="standOff"/>

                <elementSpec ident="bibl" module="core" mode="change">
                    <classes mode="replace">
                        <memberOf key="model.biblLike"/>
                        <memberOf key="model.biblPart"/>
                        <memberOf key="att.declarable"/>
                        <memberOf key="att.docStatus"/>
                        <memberOf key="att.global"/>
                        <memberOf key="att.sortable"/>
                        <memberOf key="att.timed" mode="add"/>
                        <memberOf key="att.typed"/>
                    </classes>
                    <remarks>
                        <p>Dans Istex on utilise plutôt biblStruct</p>
                    </remarks>
                </elementSpec>

                <!-- abstract -->
                <elementSpec ident="abstract" mode="change">
                    <content>
                        <alternate minOccurs="1" maxOccurs="unbounded">
                            <classRef key="model.pLike"/>
                            <classRef key="model.listLike"/>
                            <classRef key="model.headLike"/>
                        </alternate>
                    </content>
                    <exemplum xml:lang="fr">
                        <p>Cas où un abstract contient un titre</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples"
                                    ><abstract><head>Méthode</head><p>Contenu de la
                                    méthode.</p><head>Expérience</head><p>Contenu de
                                    l'expérience.</p></abstract></egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>head</gi> dans
                            <gi>abstract</gi></p>
                    </remarks>
                </elementSpec>

                <!-- listAnnotation -->
                <elementSpec ident="listAnnotation" mode="change"
                    ns="http://www.example.org/ns/nonTEI">
                    <classes>
                        <memberOf key="model.divPart.spoken"/>
                    </classes>
                    <exemplum xml:lang="fr">
                        <p>L'élément listAnnotation regroupe des <gi>annotationBlock</gi></p>
                        <p>L'élément annotationBlock regroupe un enrichissement (entité nommée,
                            catégorisation, indexation) et les éléments qui lui correspondent comme
                            les <gi>term</gi> et les <gi>fs</gi></p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="date" xml:lang="fr">
                                <annotationBlock corresp="text">
                                    <date change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                        scheme="https://date-entity.data.istex.fr">
                                        <term>2000</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="5"/>
                                            </f>
                                        </fs>
                                    </date>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="rd-nb">
                                <annotationBlock corresp="abstract">
                                    <keywords change="#rd-nb" resp="#istex-rd"
                                        scheme="https://inist-category.data.istex.fr">
                                        <term cert="0.651637563297372" key="refCode : STM" level="1"
                                            >SCIENCES APPLIQUEES, TECHNOLOGIE ET MEDECINE</term>
                                        <term cert="0.5541006144477459" key="refCode : 001"
                                            level="2">SCIENCES EXACTES ET TECHNOLOGIE</term>
                                        <term cert="0.20272618300842637" key="refCode : 001E"
                                            level="3">TERRE, OCEAN, ESPACE</term>
                                        <term cert="0.31605416143215126" key="refCode : 001E02"
                                            level="4">GEOPHYSIQUE EXTERNE</term>
                                    </keywords>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="rd-multicat">
                                <annotationBlock>
                                    <keywords change="#rd-multicat" resp="#istex-rd"
                                        scheme="https://inist-category.data.istex.fr">
                                        <term level="1">SOCIAL SCIENCE</term>
                                        <term level="2">TRANSPORTATION</term>
                                    </keywords>
                                    <keywords change="#rd-multicat" resp="#istex-rd"
                                        scheme="https://inist-category.data.istex.fr">
                                        <term level="1">SCIENCE</term>
                                        <term level="2">TRANSPORTATION SCIENCE AND TECHNOLOGY</term>
                                        <term level="3">ENGINEERING, CIVIL</term>
                                    </keywords>
                                    <keywords change="#rd-multicat" resp="#istex-rd"
                                        scheme="https://inist-category.data.istex.fr">
                                        <term level="1">ECONOMIC AND SOCIAL SCIENCE</term>
                                        <term level="2">ECONOMIC AND BUSINESS</term>
                                        <term level="3">LOGISTICS AND TRANSPORTATION</term>
                                    </keywords>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <listAnnotation type="rd-teeft">
                                <annotationBlock corresp="text">
                                    <keywords change="#rd-teeft" resp="#istex-rd">
                                        <term>
                                            <term>prepulse inhibition</term>
                                            <fs type="statistics">
                                                <f name="frequency">
                                                  <numeric value="4"/>
                                                </f>
                                                <f name="specificity">
                                                  <numeric value="1"/>
                                                </f>
                                            </fs>
                                        </term>
                                        <term>
                                            <term>decompensated inpatients</term>
                                            <fs type="statistics">
                                                <f name="frequency">
                                                  <numeric value="2"/>
                                                </f>
                                                <f name="specificity">
                                                  <numeric value="0.5"/>
                                                </f>
                                            </fs>
                                        </term>
                                    </keywords>
                                </annotationBlock>
                            </listAnnotation>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>annotationBlock</gi> dans
                                <gi>listAnnotation</gi></p>
                    </remarks>
                </elementSpec>

                <!-- annotationBlock -->
                <elementSpec ident="annotationBlock" mode="change">
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <date change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://date-entity.data.istex.fr">
                                    <term>1997</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="7"/>
                                        </f>
                                    </fs>
                                </date>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff classification alignement avec le hub de métadonnées</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="#subject-01">
                                <keywords resp="#istex" change="#istex-01">
                                    <term>Pharmacologie</term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>date</gi>, <gi>persName</gi>,
                                <gi>orgName</gi>, <gi>placeName</gi>,
                            <gi>geogName</gi>,<gi>ref</gi>, <gi>keywords</gi>, <gi>author</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- date -->
                <elementSpec ident="date" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée Date</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <date change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://date-entity.data.istex.fr">
                                    <term>1997</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="7"/>
                                        </f>
                                    </fs>
                                </date>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>date</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- persName -->
                <elementSpec ident="persName" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée persName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <persName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://persname-entity.data.istex.fr">
                                    <term>Timothy Adams</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </persName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>persName</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- orgName type="funder" ou type="provider"-->
                <elementSpec ident="orgName" mode="change">
                    <content>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff entité nommée orgName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://orgname-entity.data.istex.fr">
                                    <term>Working Party</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="2"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée orgName type="funder"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://orgnamefunder-entity.data.istex.fr">
                                    <term>Organization for Pharmaceutical Safety and Research</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée orgName type="provider"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <orgName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://orgnameprovider-entity.data.istex.fr">
                                    <term>TOMCAT at the SLS in Villigen, Switzerland</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </orgName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>orgName</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- placeName -->
                <elementSpec ident="placeName" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée placeName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <placeName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://placename-entity.data.istex.fr">
                                    <term>Geneva</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="9"/>
                                        </f>
                                    </fs>
                                </placeName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>placeName</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- geogName -->
                <elementSpec ident="geogName" mode="change">
                    <content>
                        <classRef key="model.placeNamePart"/>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée geogName</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <geogName change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://geogname-entity.data.istex.fr">
                                    <term>Mississippi River</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="2"/>
                                        </f>
                                    </fs>
                                </geogName>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>geogName</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- ref type="bibl" ou type="url" ajouter l'attribut type-->
                <elementSpec ident="ref" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée ref type="bibl"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <ref type="bibl" change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://refbibl-entity.data.istex.fr">
                                    <term>Stern et al.</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </ref>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée ref type="url"</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <ref type="url" change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://refurl-entity.data.istex.fr">
                                    <term>http://www.treas.gov/press/releases/js</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="3"/>
                                        </f>
                                    </fs>
                                </ref>
                            </annotationBlock>
                        </egXML>
                    </exemplum>

                    <!-- exemple nature
                    <cite id="n1"><jtl>Nature</jtl>
        <vid>402</vid>, <ppf> 255</ppf>ndash;<ppl>262</ppl>; <cd year="1999">1999</cd></cite>-->
                    <exemplum xml:lang="fr">
                        <p>Cas où un paragraphe contient titre de revue/livre + sa tomaison </p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <p>Originally published as <ref type="cit" xml:id="n1">
                                    <title level="j" type="main">Nature</title>
                                    <biblScope unit="vol">402</biblScope>, <biblScope unit="page"
                                        from=" 255"> 255</biblScope>ndash;<biblScope unit="page"
                                        to="262">262</biblScope>; </ref>
                            </p>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>biblScope</gi> dans
                            <gi>ref</gi></p>
                    </remarks>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>ref</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- bibl -->
                <elementSpec ident="bibl" mode="change">
                    <attList>
                        <attDef ident="scheme" mode="add">
                            <desc>désigne la liste des ontologies dans lequel l'ensemble des termes
                                concernés sont définis.</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement entité nommée bibl</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <bibl change="#Unitex-3.2.0-alpha" resp="istex-rd"
                                    scheme="https://bibl-entity.data.istex.fr">
                                    <term>GIRODET, Jean (2007). Dictionnaire des pièges et
                                        difficults de la langue française, Paris : Bordas</term>
                                    <fs type="statistics">
                                        <f name="frequency">
                                            <numeric value="1"/>
                                        </f>
                                    </fs>
                                </bibl>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <remarks>
                        <p>Modèle modifié pour bénéficier de <gi>geogName</gi> dans
                                <gi>annotationBlock</gi></p>
                    </remarks>
                </elementSpec>

                <!-- SG - keywords hub de mta-->
                <elementSpec ident="keywords" mode="change">
                    <classes mode="change">
                        <memberOf key="model.annotation" mode="add"/>
                    </classes>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement subject hub de métadonnées Abes</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="#subject-01">
                                <keywords resp="#abes" change="#abes-01">
                                    <term>Pharmacologie</term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="fr">
                        <p>StandOff enrichissement indexation rd-teeft</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="text">
                                <keywords change="#istex-rd" resp="#istex-rd">
                                    <term>
                                        <term>prepulse inhibition</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="4"/>
                                            </f>
                                            <f name="specificity">
                                                <numeric value="1"/>
                                            </f>
                                        </fs>
                                    </term>
                                    <term>
                                        <term>decompensated inpatients</term>
                                        <fs type="statistics">
                                            <f name="frequency">
                                                <numeric value="2"/>
                                            </f>
                                            <f name="specificity">
                                                <numeric value="0.5"/>
                                            </f>
                                        </fs>
                                    </term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="en">
                        <p>StandOff enrichissement categorisation rd-nb</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock corresp="abstract">
                                <keywords change="#rd-nb" resp="#istex-rd"
                                    scheme="https://inist-category.data.istex.fr">
                                    <term cert="0.651637563297372" key="refCode : STM" level="1"
                                        >SCIENCES APPLIQUEES, TECHNOLOGIE ET MEDECINE</term>
                                    <term cert="0.5541006144477459" key="refCode : 001" level="2"
                                        >SCIENCES EXACTES ET TECHNOLOGIE</term>
                                    <term cert="0.20272618300842637" key="refCode : 001E" level="3"
                                        >TERRE, OCEAN, ESPACE</term>
                                    <term cert="0.31605416143215126" key="refCode : 001E02"
                                        level="4">GEOPHYSIQUE EXTERNE</term>
                                </keywords>
                            </annotationBlock>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="en">
                        <p>StandOff enrichissement categorisation rd-multicat</p>
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <annotationBlock>
                                <keywords change="#rd-multicat" resp="#istex-rd"
                                    scheme="https://inist-category.data.istex.fr">
                                    <term level="1">SOCIAL SCIENCE</term>
                                    <term level="2">TRANSPORTATION</term>
                                </keywords>
                                <keywords change="#rd-multicat" resp="#istex-rd"

tei_standOff =
  
  ## Container element for stand-off annotations embedded in a TEI document
  element ns2:standOff {
    tei_teiHeader?,
    tei_model.resourceLike*,
    (tei_listAnnotation*)
    >> sch:ns [ prefix = "stdf" uri = "http://standoff.proposal" ]
    >> sch:pattern [
         id =
           "tei-istex-standOff-SecondLevelStandOffTyping-constraint-assert-18"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "stdf:standOff"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@type or not(ancestor::stdf:standOff)"
             "This "
             sch:name [ ]
             " element must have a @type attribute, since it has an ancestor\x{a}" ~
             "                                "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.typed.attributes,
    tei_att.notated.attributes,
    tei_att.datable.w3c.attributes,
    tei_att.pointing.attributes,
    empty
  }
tei_listAnnotation =
  
  ## Groups together various annotations, for instance for parallel interpretations of a spoken segment
  element ns2:listAnnotation {
    (tei_model.headLike*, tei_model.annotation+),
    tei_att.global.attributes,
    tei_att.typed.attributes,
    tei_att.notated.attributes,
    tei_att.timed.attributes,
    tei_att.declaring.attributes,
    empty
  }
tei_att.msExcerpt.attributes = tei_att.msExcerpt.attribute.defective
tei_att.msExcerpt.attribute.defective =
  
  ## indique si le passage décrit est fautif, i.e. incomplet en raison d'une lacune ou d'une détérioration.
  [ a:defaultValue = "false" ]
  attribute defective {
    xsd:boolean
    | (
       ##
       "unknown"
       | 
         ##
         "inapplicable")
  }?
tei_att.msClass.attributes = tei_att.msClass.attribute.class
tei_att.msClass.attribute.class =
  
  ## spécifie la ou les catégories ou classes auxquelles l'item appartient.

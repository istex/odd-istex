tei_s =
  
  ## (phrase) contient une division textuelle de type phrase [17.1. Linguistic Segment Categories 8.4.1. Segmentation]
  element tei:s {
    tei_macro.phraseSeq
    >> sch:pattern [
         id = "tei-istex-s-noNestedS-constraint-report-19"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:s"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:s"
             "You may not nest one s element within\x{a}" ~
             "      another: use seg instead"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
